class Soldier(GameObject):
def __init__(self,name):
        self.class_name="Jecky"
        self.health = 4
        self._desc = " A foul creature "
        super().__init__(name)
              
        @property
def desc(self)
      if self.healf >= 4:
      return self._desc
              
      elif self.health == 3:
                health_line = "It has a wound on its nose"
      elif self.health == 2:
              health_line = "Its left arm has been cut off!"
      elif self.health == 1:
              health_line = "Its right ear has been cut off!"
      elif self.health <= 0:
              health_line="Its dead"
          return self._desc + "\n" + health_line

        @desc.setter
def desc(self,value):
           self._desc = value
def hit(noun)
      if noun in GameObject.objects:
